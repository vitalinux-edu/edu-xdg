<!DOCTYPE Menu PUBLIC "-//freedesktop//DTD Menu 1.0//EN"
 "http://www.freedesktop.org/standards/menu-spec/1.0/menu.dtd">

<Menu>
    <Name>Applications</Name>
    <Directory>X-GNOME-Menu-Applications.directory</Directory>

    <!-- Scan legacy dirs first, as later items take priority -->
    <LegacyDir>/etc/X11/applnk</LegacyDir>
    <LegacyDir>/usr/share/gnome/apps</LegacyDir>

    <!-- Read standard .directory and .desktop file locations -->
    <DefaultAppDirs/>
    <DefaultDirectoryDirs/>

    <!-- Read in overrides and child menus from applications-merged/ -->
    <DefaultMergeDirs/>

    <Menu>
        <Name>System</Name>
        <Directory>System-Tools.directory</Directory>
        <Include>
            <And>
                <Category>System</Category>
                <Not><Category>Settings</Category></Not>
                <Not><Category>Game</Category></Not>
            </And>
            <Filename>ubuntu-software-center.desktop</Filename>
        </Include>
        <Exclude>
                <Filename>redhat-usermount.desktop</Filename>
                <Filename>baobab.desktop</Filename>
                <Filename>dconf-editor.desktop</Filename>
                <Filename>gdebi.desktop</Filename>
                <Filename>gnome-power-statistics.desktop</Filename>
                <Filename>gnome-system-log.desktop</Filename>
                <Filename>gnome-system-monitor.desktop</Filename>
                <Filename>ibus.desktop</Filename>
                <Filename>kde4-nepomukbackup.desktop</Filename>
                <Filename>kde4-nepomukcontroller.desktop</Filename>
        </Exclude>
        <Menu>
            <Name>Administration</Name>
            <Directory>Settings-System.directory</Directory>
            <Include>
                <And>
                    <Category>Settings</Category>
                    <Category>System</Category>
                    <Not>
                        <Category>X-GNOME-Settings-Panel</Category>
                    </Not>
                </And>
                <Filename>migasfree-launcher.desktop</Filename>
                <Filename>migasfree-label.desktop</Filename>
                <Filename>redhat-usermount.desktop</Filename>
                <Filename>baobab.desktop</Filename>
                <Filename>nm-connection-editor.desktop</Filename>
                <Filename>onboard-settings.desktop</Filename>
                <Filename>redhat-userpasswd.desktop</Filename>
                <Filename>kde4-nepomukbackup.desktop</Filename>
                <Filename>usb-creator-gtk.desktop</Filename>
                <Filename>dconf-editor.desktop</Filename>
                <Filename>qparted.desktop</Filename>
                <Filename>update-manager.desktop</Filename>
                <Filename>synaptic.desktop</Filename>
                <Filename>gnome-nettool.desktop</Filename>
                <Filename>system-config-printer.desktop</Filename>
                <Filename>ubiquity-gtkui.desktop</Filename>
                <Filename>gnome-system-monitor.desktop</Filename>
                <Filename>software-properties-gtk.desktop</Filename>
                <Filename>remastersys-gui.desktop</Filename>
                <Filename>palimpsest.desktop</Filename>
                <Filename>gnome-system-log.desktop</Filename>
            </Include>
            <Exclude>
                <Filename>ubuntu-software-center.desktop</Filename>
                <Filename>ibus.desktop</Filename>
                <Filename>checkbox-qt.desktop</Filename>
            </Exclude>
        </Menu>
        <Menu>
            <Name>Preferences</Name>
            <Directory>Settings.directory</Directory>
            <Include>
                <And>
                    <Category>Settings</Category>
                    <Not>
                        <Or>
                            <Category>System</Category>
