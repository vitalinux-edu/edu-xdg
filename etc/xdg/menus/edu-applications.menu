            </Include>
            <Exclude>
                <Filename>ibus-setup.desktop</Filename>
                <Filename>nm-connection-editor.desktop</Filename>
                <Filename>palimpsest.desktop</Filename>
                <Filename>redhat-userpasswd.desktop</Filename>
            </Exclude>
